{
  "name": "create-app",
  "description": "Generate a new app",
  "version": "0.0.6",
  "keywords": [],
  "author": "Paul Heintzelman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/pheintzelman/create-app"
  },
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "coverage": "open ./tests/coverage/lcov-report/index.html"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "transform": {},
    "testMatch": [
      "**/*.test.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/*.test.js",
      "!**/templates/**",
      "!**/tests/**"
    ],
    "coverageDirectory": "./tests/coverage"
  },
  "bin": {
    "create-app": "./src/cli/cli.js"
  },
  "dependencies": {
    "mustache": "^4.2.0",
    "pino": "^6.11.3",
    "pino-pretty": "^5.0.2",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "jest": "^27.0.4",
    "open": "^8.2.0"
  }
}
