{
  "name": "react-app",
  "version": "0.0.1",
  "options": {
    "ui": ["react"],
    "api": ["stub"],
    "db": []
  },
  "directives": [
    { "ui:react": { "copy": { "src": "react/base", "dest": "" } } },
    {
      "ui:react": {
        "loop": {
          "property": "models",
          "src": "react/loop",
          "dest": "src/components"
        }
      }
    },
    {
      "api:stub": {
        "loop": {
          "property": "models",
          "src": "api/stub/api",
          "dest": "src/api"
        }
      }
    }
  ]
}
